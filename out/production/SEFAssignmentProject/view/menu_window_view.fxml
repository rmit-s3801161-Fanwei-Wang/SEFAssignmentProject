<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="161.0" prefWidth="145.0" spacing="5.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.MenuWindowViewController">
   <children>
      <Label alignment="CENTER" prefHeight="38.0" prefWidth="125.0" text="Menu" />
      <Button mnemonicParsing="false" onAction="#loadGame" prefHeight="30.0" prefWidth="100.0" text="Load Game" />
      <Button mnemonicParsing="false" onAction="#createGame" prefHeight="30.0" prefWidth="100.0" text="Creat Game" />
      <Button mnemonicParsing="false" onAction="#logout" prefHeight="30.0" prefWidth="100.0" text="Log out" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
